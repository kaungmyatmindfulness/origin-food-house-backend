{
  "name": "origin-food-house-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "NODE_ENV=dev nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate:db": "prisma migrate dev",
    "generate:db": "prisma generate",
    "studio:db": "prisma studio",
    "reset:db": "prisma migrate reset --force",
    "seed:db": "prisma db seed",
    "drop:db": "prisma migrate reset --force && prisma db push --force",
    "sync:postman": "node sync-postman.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.782.0",
    "@faker-js/faker": "^9.6.0",
    "@nestjs/common": "^11.0.14",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.14",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.14",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^11.1.1",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.6.0",
    "bcrypt": "^5.1.1",
    "chalk": "^5.4.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "disposable-email-domains": "^1.0.62",
    "dotenv": "^16.4.7",
    "multer": "^2.0.1",
    "nanoid": "^5.1.5",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sharp": "^0.34.1",
    "slugify": "^1.6.6",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "@nestjs/cli": "^11.0.6",
    "@nestjs/schematics": "^11.0.4",
    "@nestjs/testing": "^11.0.14",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.18",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/disposable-email-domains": "^1.0.6",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.14.0",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "prisma": "^6.6.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.1",
    "ts-loader": "^9.5.2",
